ВЫБРАТЬ РАЗРЕШЕННЫЕ
	МИНИМУМ(ДоговораНаДату.Период) КАК Период,
	ДоговораНаДату.Договор КАК Договор,
	&amp;ДатаСреза
ПОМЕСТИТЬ Минимум_Подготовка
ИЗ
	РегистрСведений.абс_ИзменениеСтатусовДоговоров КАК ДоговораНаДату
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ДоговораПоПроектам КАК ДоговораПоПроектам
		ПО ДоговораНаДату.Договор = ДоговораПоПроектам.Договор
ГДЕ
	ДоговораНаДату.СтатусДоговора В (ЗНАЧЕНИЕ(Перечисление.абсСтатусыДоговоров.Подготовка))
	И ДоговораНаДату.Период &lt;= КОНЕЦПЕРИОДА(&amp;ДатаСреза, МЕСЯЦ)

СГРУППИРОВАТЬ ПО
	ДоговораНаДату.Договор

ИНДЕКСИРОВАТЬ ПО
	Договор
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	МИНИМУМ(ДоговораНаДату_Исполнение.Период) КАК Период,
	ДоговораНаДату_Исполнение.Договор КАК Договор,
	&amp;ДатаСреза
ПОМЕСТИТЬ Минимум_Исполнение
ИЗ
	РегистрСведений.абс_ИзменениеСтатусовДоговоров КАК ДоговораНаДату_Исполнение
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ ДоговораПоПроектам КАК ДоговораПоПроектам
		ПО ДоговораНаДату_Исполнение.Договор = ДоговораПоПроектам.Договор
ГДЕ
	ДоговораНаДату_Исполнение.СтатусДоговора = ЗНАЧЕНИЕ(Перечисление.абсСтатусыДоговоров.Исполнение)
	И ДоговораНаДату_Исполнение.Период &lt;= КОНЕЦПЕРИОДА(&amp;ДатаСреза, МЕСЯЦ)

СГРУППИРОВАТЬ ПО
	ДоговораНаДату_Исполнение.Договор

ИНДЕКСИРОВАТЬ ПО
	Договор
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Минимум_Подготовка.Период,
	Минимум_Подготовка.Договор КАК Договор,
	&amp;ДатаСреза,
	1 КАК Новые,
	0 КАК Подписанные,
	0 КАК ПереходящиеСПредыдущегоПериода
ПОМЕСТИТЬ ВТ
ИЗ
	Минимум_Подготовка КАК Минимум_Подготовка
ГДЕ
	Минимум_Подготовка.Период &lt;= КОНЕЦПЕРИОДА(&amp;ДатаСреза, МЕСЯЦ)
	И Минимум_Подготовка.Период &gt;= НАЧАЛОПЕРИОДА(&amp;ДатаСреза, МЕСЯЦ)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Минимум_Исполнение.Период,
	Минимум_Исполнение.Договор,
	&amp;ДатаСреза,
	0,
	1,
	0
ИЗ
	Минимум_Исполнение КАК Минимум_Исполнение
ГДЕ
	Минимум_Исполнение.Период &lt;= КОНЕЦПЕРИОДА(&amp;ДатаСреза, МЕСЯЦ)
	И Минимум_Исполнение.Период &gt;= НАЧАЛОПЕРИОДА(&amp;ДатаСреза, МЕСЯЦ)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Минимум_Подготовка.Период,
	Минимум_Подготовка.Договор,
	&amp;ДатаСреза,
	0,
	0,
	ВЫБОР
		КОГДА НАЧАЛОПЕРИОДА(Минимум_Подготовка.Период, МЕСЯЦ) &lt; НАЧАЛОПЕРИОДА(&amp;ДатаСреза, МЕСЯЦ)
				И НАЧАЛОПЕРИОДА(ЕСТЬNULL(Минимум_Исполнение.Период, КОНЕЦПЕРИОДА(&amp;ДатаСреза, МЕСЯЦ)), МЕСЯЦ) &gt;= НАЧАЛОПЕРИОДА(&amp;ДатаСреза, МЕСЯЦ)
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ
ИЗ
	Минимум_Подготовка КАК Минимум_Подготовка
		ЛЕВОЕ СОЕДИНЕНИЕ Минимум_Исполнение КАК Минимум_Исполнение
		ПО Минимум_Подготовка.Договор = Минимум_Исполнение.Договор

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Минимум_Исполнение.Период,
	Минимум_Исполнение.Договор,
	&amp;ДатаСреза,
	1,
	0,
	0
ИЗ
	Минимум_Исполнение КАК Минимум_Исполнение
		ЛЕВОЕ СОЕДИНЕНИЕ Минимум_Подготовка КАК Минимум_Подготовка
		ПО (Минимум_Подготовка.Договор = Минимум_Исполнение.Договор)
ГДЕ
	Минимум_Исполнение.Период &lt;= КОНЕЦПЕРИОДА(&amp;ДатаСреза, МЕСЯЦ)
	И Минимум_Исполнение.Период &gt;= НАЧАЛОПЕРИОДА(&amp;ДатаСреза, МЕСЯЦ)
	И Минимум_Подготовка.Договор ЕСТЬ NULL 
;

