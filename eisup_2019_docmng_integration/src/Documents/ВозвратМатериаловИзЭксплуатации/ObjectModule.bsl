// Процедура определяет параметры учетной политики
//
Процедура ПодготовитьПараметрыУчетнойПолитики(Отказ)

	Если ОтражатьВУправленческомУчете Тогда
		
		УчетнаяПолитикаУпр = ttk_ОбщегоНазначения.ПолучитьПараметрыУчетнойПолитикиУпр(Дата);
	    Если НЕ ЗначениеЗаполнено(УчетнаяПолитикаУпр) Тогда
			Отказ = Истина;
		КонецЕсли;
		
	КонецЕсли;
	
	Если ОтражатьВБухгалтерскомУчете Тогда
		
		УчетнаяПолитикаРегл = ttk_ОбщегоНазначения.ПолучитьПараметрыУчетнойПолитикиРегл(Дата, Организация);
	    Если НЕ ЗначениеЗаполнено(УчетнаяПолитикаРегл) Тогда
			Отказ = Истина;
		КонецЕсли;
		
	КонецЕсли;
	
КонецПроцедуры // ПодготовитьПараметрыУчетнойПолитики()

// Процедура формирует структуру шапки документа и дополнительных полей.
//
Процедура ПодготовитьСтруктуруШапкиДокумента(Заголовок, СтруктураШапкиДокумента, Отказ = Ложь) Экспорт
	
	СтруктураШапкиДокумента = ПроверкаРеквизитов(Отказ);
	
	// Заполним по шапке документа дерево параметров, нужных при проведении.
	ДеревоПолейЗапросаПоШапке = УправлениеЗапасами.СформироватьДеревоПолейЗапросаПоШапке();
	УправлениеЗапасами.ДобавитьСтрокуВДеревоПолейЗапросаПоШапке(ДеревоПолейЗапросаПоШапке, "УчетнаяПолитика",  "СписыватьПартииПриПроведенииДокументов",     "СписыватьПартииПриПроведенииДокументов");
	УправлениеЗапасами.ДобавитьСтрокуВДеревоПолейЗапросаПоШапке(ДеревоПолейЗапросаПоШапке, "УчетнаяПолитика",  "ВестиПартионныйУчетПоСкладам",               "ВестиПартионныйУчетПоСкладам");
	УправлениеЗапасами.ДобавитьСтрокуВДеревоПолейЗапросаПоШапке(ДеревоПолейЗапросаПоШапке, "УчетнаяПолитика",  "ВестиУчетТоваровОрганизацийВРазрезеСкладов", "ВестиУчетТоваровОрганизацийВРазрезеСкладов");
	УправлениеЗапасами.ДобавитьСтрокуВДеревоПолейЗапросаПоШапке(ДеревоПолейЗапросаПоШапке, "Склад",            "ВидСклада",                                  "ВидСклада");

	// Сформируем запрос на дополнительные параметры, нужные при проведении, по данным шапки документа
	СтруктураШапкиДокумента = УправлениеЗапасами.СформироватьЗапросПоДеревуПолей(ЭтотОбъект, ДеревоПолейЗапросаПоШапке, СтруктураШапкиДокумента, мВалютаРегламентированногоУчета);
	
	ИспользоватьРасширеннуюАналитику = глЗначениеПеременной("ИспользоватьРасширеннуюАналитикуУчетаНоменклатурыИЗатрат") 
		И (глЗначениеПеременной("ДатаНачалаИспользованияРасширеннойАналитикиУчетаНоменклатурыИЗатрат") <= Дата);
	СтруктураШапкиДокумента.Вставить("ИспользоватьРасширеннуюАналитику", ИспользоватьРасширеннуюАналитику);
	
	Заголовок = ttk_ОбщегоНазначения.ПредставлениеДокументаПриПроведении(Ссылка);
	
КонецПроцедуры // ПодготовитьСтруктуруШапкиДокумента()

// Процедура формирует таблицы документа.
//
Процедура ПодготовитьТаблицыДокумента(СтруктураШапкиДокумента, ТаблицаПоМатериалам) Экспорт
	
	// Подготовим таблицу материалов для проведения.
	СтруктураПолей = Новый Структура();
	СтруктураПолей.Вставить("Номенклатура"        				, "Номенклатура");
	СтруктураПолей.Вставить("Услуга"              				, "Номенклатура.Услуга");
	СтруктураПолей.Вставить("Набор"               				, "Номенклатура.Набор");
	СтруктураПолей.Вставить("ВестиПартионныйУчетПоСериям"		, "Номенклатура.ВестиПартионныйУчетПоСериям");
	СтруктураПолей.Вставить("Количество"          				, "Количество * Коэффициент /Номенклатура.ЕдиницаХраненияОстатков.Коэффициент");
	
	СтруктураПолей.Вставить("ТипСтоимости"						, "ТипСтоимости");
	СтруктураПолей.Вставить("Стоимость"							, "Сумма");
	СтруктураПолей.Вставить("СтоимостьРегл"						, "СуммаРегл");
	
	СтруктураПолей.Вставить("ХарактеристикаНоменклатуры"		, "ХарактеристикаНоменклатуры");
	СтруктураПолей.Вставить("СерияНоменклатуры"         		, "СерияНоменклатуры");
	
	СтруктураПолей.Вставить("ФизЛицо"         					, "ФизЛицо");
	СтруктураПолей.Вставить("НазначениеИспользования"         	, "НазначениеИспользования");
	
	СтруктураПолей.Вставить("Качество"         					, "Качество");
	СтруктураПолей.Вставить("ДокументПередачи"         			, "ДокументПередачи");
	
	ДополнитьСтруктуруПолейТабличнойЧастиМатериалыРегл(СтруктураПолей);
	
	РезультатЗапросаПоМатериалам = УправлениеЗапасами.СформироватьЗапросПоТабличнойЧасти(ЭтотОбъект, "Материалы", СтруктураПолей);
	ТаблицаПоМатериалам = ПодготовитьТаблицуМатериалов(РезультатЗапросаПоМатериалам, СтруктураШапкиДокумента);
	
	//Заполнение счетов учета номенклатуры и затрат
	СчетаУчетаВДокументах.ЗаполнитьПриПроведенииСчетаУчетаТабличнойЧасти("Материалы", ТаблицаПоМатериалам, 	СтруктураШапкиДокумента);
	
	Если СтруктураШапкиДокумента.ОтражатьВБухгалтерскомУчете Тогда
		БухгалтерскийУчетРасчетовСКонтрагентами.ПодготовкаТаблицыЗначенийДляЦелейПриобретенияИРеализации(ТаблицаПоМатериалам, СтруктураШапкиДокумента, Ложь);
	КонецЕсли;
	
КонецПроцедуры // СформироватьТаблицыДокумента()

// Процедура - обработчик события ОбработкаПроведения
//
Процедура ОбработкаПроведения(Отказ, РежимПроведения)
	
	Перем Заголовок, СтруктураШапкиДокумента, ТаблицаПоМатериалам;
	
	Если мУдалятьДвижения Тогда
		ttk_ОбщегоНазначения.УдалитьДвиженияРегистратора(ЭтотОбъект, Отказ);
	КонецЕсли;

	ПодготовитьПараметрыУчетнойПолитики(Отказ);
	
	Если Отказ Тогда
		Возврат;
	КонецЕсли;
	
	ПодготовитьСтруктуруШапкиДокумента(Заголовок, СтруктураШапкиДокумента, Отказ);
	
	ПодготовитьТаблицыДокумента(СтруктураШапкиДокумента, ТаблицаПоМатериалам);
	
	//Заполнение и проверка заполнения счетов учета номенклатуры и затрат
	СчетаУчетаВДокументах.ПроверитьСчетаУчетаТабличнойЧасти("Материалы", ТаблицаПоМатериалам, 	СтруктураШапкиДокумента, Отказ, Заголовок);
	
	// Движения по документу.
	Если Не Отказ Тогда
		ДвиженияПоРегистрам(РежимПроведения, СтруктураШапкиДокумента, ТаблицаПоМатериалам, Отказ, Заголовок);
	КонецЕсли;
	
	
	//Сделаем переменные доступными из подписок на события
	ДополнительныеСвойства.Вставить("СтруктураШапкиДокумента", СтруктураШапкиДокумента);
	ДополнительныеСвойства.Вставить("СтруктураТабличныхЧастей", Новый Структура("ТаблицаПоМатериалам", ТаблицаПоМатериалам));
	
КонецПроцедуры	// ОбработкаПроведения()

// Процедура - обработчик события ОбработкаЗаполнения
//
Процедура ОбработкаЗаполнения(Основание)
	Если ТипЗнч(Основание) = Тип("ДокументСсылка.ПередачаМатериаловВЭксплуатацию") Тогда
		
		// Заполнение шапки
		ОтражатьВБухгалтерскомУчете  = Основание.ОтражатьВБухгалтерскомУчете;
		ОтражатьВНалоговомУчете      = Основание.ОтражатьВНалоговомУчете;
		ОтражатьВУправленческомУчете = Основание.ОтражатьВУправленческомУчете;
		Организация                  = Основание.Организация;
		Подразделение                = Основание.Подразделение;
		ПодразделениеОрганизации     = Основание.ПодразделениеОрганизации;
		Комментарий   = Основание.Комментарий;
		Ответственный = Основание.Ответственный;
		Склад         = Основание.Склад;
		
		Для Каждого ТекСтрокаМатериалы Из Основание.Материалы Цикл
			
			НоваяСтрока = Материалы.Добавить();
			
			НоваяСтрока.Номенклатура               = ТекСтрокаМатериалы.Номенклатура;
			НоваяСтрока.ХарактеристикаНоменклатуры = ТекСтрокаМатериалы.ХарактеристикаНоменклатуры;
			НоваяСтрока.СерияНоменклатуры          = ТекСтрокаМатериалы.СерияНоменклатуры;
			
			НоваяСтрока.ЕдиницаИзмерения           = ТекСтрокаМатериалы.ЕдиницаИзмерения;
			НоваяСтрока.ЕдиницаИзмеренияМест       = ТекСтрокаМатериалы.ЕдиницаИзмеренияМест;
			НоваяСтрока.Количество                 = ТекСтрокаМатериалы.Количество;
			НоваяСтрока.КоличествоМест             = ТекСтрокаМатериалы.КоличествоМест;
			НоваяСтрока.Коэффициент                = ТекСтрокаМатериалы.Коэффициент;
			НоваяСтрока.Качество                   = ТекСтрокаМатериалы.Качество;
			
			НоваяСтрока.СчетУчетаБУ                = ТекСтрокаМатериалы.СчетУчетаБУ;
			НоваяСтрока.СчетПередачиБУ             = ТекСтрокаМатериалы.СчетПередачиБУ;
			НоваяСтрока.СчетУчетаНУ                = ТекСтрокаМатериалы.СчетУчетаНУ;
			НоваяСтрока.СчетПередачиНУ             = ТекСтрокаМатериалы.СчетПередачиНУ;
			
			НоваяСтрока.ФизЛицо 				   = ТекСтрокаМатериалы.ФизЛицо;
			НоваяСтрока.НазначениеИспользования    = ТекСтрокаМатериалы.НазначениеИспользования;
			
			НоваяСтрока.ТипСтоимости = Перечисления.ВидыНормативнойСтоимостиПроизводства.Рассчитывается;
			НоваяСтрока.ДокументПередачи = Основание;
			
		КонецЦикла;
		
	КонецЕсли;
	
КонецПроцедуры // ОбработкаЗаполнения()

Процедура ПередЗаписью(Отказ, РежимЗаписи, РежимПроведения)
	Если ОбменДанными.Загрузка  Тогда
		Возврат;
	КонецЕсли;


	 
	мУдалятьДвижения = НЕ ЭтоНовый();
	
КонецПроцедуры // ПередЗаписью

Процедура ПриЗаписи(Отказ)
	Если ОбменДанными.Загрузка  Тогда
		Возврат;
	КонецЕсли;	
КонецПроцедуры

Процедура абсДвиженияПоРегистрамПартийМатериаловВЭксплуатации(СтруктураШапкиДокумента, ТаблицаПоМатериалам, Отказ, Заголовок) //АБС+ Коломиец 20856
	
	СтруктураРазделовУчета = Новый Структура;
	СтруктураРазделовУчета.Вставить("БУ", "БухгалтерскийУчет");
	СтруктураРазделовУчета.Вставить("НУ", "НалоговыйУчет");
	
	СтруктураСоответствияРазделовУчетаДокументу = Новый Структура;
	СтруктураСоответствияРазделовУчетаДокументу.Вставить("УУ", "ОтражатьВУправленческомУчете");
	СтруктураСоответствияРазделовУчетаДокументу.Вставить("БУ", "ОтражатьВБухгалтерскомУчете");
	СтруктураСоответствияРазделовУчетаДокументу.Вставить("НУ", "ОтражатьВНалоговомУчете");
	
	Для Каждого РазделУчета Из СтруктураРазделовУчета Цикл
		
		Если СтруктураШапкиДокумента[СтруктураСоответствияРазделовУчетаДокументу[РазделУчета.Ключ]] = Ложь Тогда
			Продолжить;
		КонецЕсли;
		
		ЭтоНУ = РазделУчета.Ключ = "НУ";
		
		Запрос = Новый Запрос;
		Запрос.МенеджерВременныхТаблиц = Новый МенеджерВременныхТаблиц;
		
		Запрос.Текст = "ВЫБРАТЬ
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.Номенклатура,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.ХарактеристикаНоменклатуры,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.СерияНоменклатуры,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.ДокументПередачи,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.ФизЛицо,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.НазначениеИспользования,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.НазначениеИспользования.СпособПогашенияСтоимости КАК СпособПогашенияСтоимости,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.НазначениеИспользования.СрокПолезногоИспользования КАК СрокПолезногоИспользования,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.Количество,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.Сумма,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.НомерСтроки,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.Ссылка.Организация,
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.СчетПередачиБУ КАК СчетУчета
		               |ПОМЕСТИТЬ ВТ_Материалы
		               |ИЗ
		               |	Документ.ВозвратМатериаловИзЭксплуатации.Материалы КАК ВозвратМатериаловИзЭксплуатацииМатериалы
		               |ГДЕ
		               |	ВозвратМатериаловИзЭксплуатацииМатериалы.Ссылка = &Ссылка
		               |;
		               |
		               |////////////////////////////////////////////////////////////////////////////////
		               |ВЫБРАТЬ
		               |	ВТ_Материалы.Номенклатура,
		               |	ВТ_Материалы.ХарактеристикаНоменклатуры,
		               |	ВТ_Материалы.СерияНоменклатуры,
		               |	ВТ_Материалы.ДокументПередачи,
		               |	ВТ_Материалы.ФизЛицо,
		               |	ВТ_Материалы.НазначениеИспользования,
		               |	СУММА(ВТ_Материалы.Количество) КАК Количество,
		               |	СУММА(ВТ_Материалы.Сумма) КАК Сумма,
		               |	ВТ_Материалы.Организация,
		               |	ВТ_Материалы.СчетУчета
		               |ПОМЕСТИТЬ ВТ_МатериалыСвернуто
		               |ИЗ
		               |	ВТ_Материалы КАК ВТ_Материалы
		               |
		               |СГРУППИРОВАТЬ ПО
		               |	ВТ_Материалы.НазначениеИспользования,
		               |	ВТ_Материалы.ФизЛицо,
		               |	ВТ_Материалы.ДокументПередачи,
		               |	ВТ_Материалы.СерияНоменклатуры,
		               |	ВТ_Материалы.СчетУчета,
		               |	ВТ_Материалы.Организация,
		               |	ВТ_Материалы.ХарактеристикаНоменклатуры,
		               |	ВТ_Материалы.Номенклатура
		               |;
		               |
		               |////////////////////////////////////////////////////////////////////////////////
		               |ВЫБРАТЬ
		               |	ВТ_МатериалыСвернуто.Номенклатура,
		               |	ВТ_МатериалыСвернуто.ХарактеристикаНоменклатуры,
		               |	ВТ_МатериалыСвернуто.СерияНоменклатуры,
		               |	ПартииМатериаловВЭксплуатацииОстатки.ДокументПередачи КАК ДокументПередачи,
		               |	ВТ_МатериалыСвернуто.ФизЛицо,
		               |	ВТ_МатериалыСвернуто.НазначениеИспользования,
		               |	ВТ_МатериалыСвернуто.Организация,
		               |	ВТ_МатериалыСвернуто.СчетУчета,
		               |	ЕСТЬNULL(ПартииМатериаловВЭксплуатацииОстатки.КоличествоОстаток, 0) КАК КоличествоОстаток,
		               |	ЕСТЬNULL(ПартииМатериаловВЭксплуатацииОстатки.СтоимостьОстаток, 0) КАК СтоимостьОстаток,
					   | "+ ?(ЭтоНУ, "		
					   |ЕСТЬNULL(ПартииМатериаловВЭксплуатацииОстатки.ПостояннаяРазницаОстаток, 0) КАК ПостояннаяРазницаОстаток,
					   |ЕСТЬNULL(ПартииМатериаловВЭксплуатацииОстатки.ВременнаяРазницаОстаток, 0) КАК ВременнаяРазницаОстаток,
					   |ЕСТЬNULL(ПартииМатериаловВЭксплуатацииОстатки.ПогашеннаяСтоимостьПРОстаток, 0) КАК ПогашеннаяСтоимостьПРОстаток,
					   |ЕСТЬNULL(ПартииМатериаловВЭксплуатацииОстатки.ПогашеннаяСтоимостьВРОстаток, 0) КАК ПогашеннаяСтоимостьВРОстаток,
					   | " , "
					   |	ЕСТЬNULL(ПартииМатериаловВЭксплуатацииОстатки.ПогашеннаяСтоимостьОстаток, 0) КАК ПогашеннаяСтоимостьОстаток," ) +"
		               |	&Подразделение,
		               |	ВТ_МатериалыСвернуто.ДокументПередачи КАК ДокументПередачиТЧ
		               |ИЗ
		               |	ВТ_МатериалыСвернуто КАК ВТ_МатериалыСвернуто
		               |		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ПартииМатериаловВЭксплуатации"+ РазделУчета.Значение+
					   ".Остатки(
		               |				&Период,
		               |				Подразделение = &Подразделение
		               |					И Организация = &Организация) КАК ПартииМатериаловВЭксплуатацииОстатки
		               |		ПО ВТ_МатериалыСвернуто.Организация = ПартииМатериаловВЭксплуатацииОстатки.Организация
		               |			И ВТ_МатериалыСвернуто.Номенклатура = ПартииМатериаловВЭксплуатацииОстатки.Номенклатура
		               |			И ВТ_МатериалыСвернуто.ХарактеристикаНоменклатуры = ПартииМатериаловВЭксплуатацииОстатки.ХарактеристикаНоменклатуры
		               |			И ВТ_МатериалыСвернуто.СерияНоменклатуры = ПартииМатериаловВЭксплуатацииОстатки.СерияНоменклатуры
		               |			И ВТ_МатериалыСвернуто.ФизЛицо = ПартииМатериаловВЭксплуатацииОстатки.ФизЛицо
		               |			И ВТ_МатериалыСвернуто.НазначениеИспользования = ПартииМатериаловВЭксплуатацииОстатки.НазначениеИспользования
		               |			И ВТ_МатериалыСвернуто.СчетУчета = ПартииМатериаловВЭксплуатацииОстатки.СчетУчета
		               |			И (ВЫБОР
		               |				КОГДА ВТ_МатериалыСвернуто.ДокументПередачи = ЗНАЧЕНИЕ(Документ.ПередачаМатериаловВЭксплуатацию.ПустаяСсылка)
		               |						ИЛИ ВТ_МатериалыСвернуто.ДокументПередачи = НЕОПРЕДЕЛЕНО
		               |						ИЛИ ВТ_МатериалыСвернуто.ДокументПередачи ЕСТЬ NULL 
		               |					ТОГДА ИСТИНА
		               |				ИНАЧЕ ВТ_МатериалыСвернуто.ДокументПередачи = ПартииМатериаловВЭксплуатацииОстатки.ДокументПередачи
		               |			КОНЕЦ)";
		
		
		Если ЭтоНУ Тогда
			Запрос.Текст = СтрЗаменить(Запрос.Текст, "СчетПередачиБУ", "СчетПередачиНУ");
		КонецЕсли;
		
		Запрос.УстановитьПараметр("Ссылка", Ссылка);
		Запрос.УстановитьПараметр("Организация", СтруктураШапкиДокумента.Организация);
		Запрос.УстановитьПараметр("Подразделение", СтруктураШапкиДокумента.ПодразделениеОрганизации);
		Запрос.УстановитьПараметр("Период", Новый Граница(СтруктураШапкиДокумента.Ссылка.МоментВремени(), ВидГраницы.Исключая));
		
		ОстаткиПоРегистру = Запрос.Выполнить().Выгрузить();
		
		//если остатков достаточно
		Запрос.Текст = "ВЫБРАТЬ
		|	ВТ_Материалы.Номенклатура,
		|	ВТ_Материалы.ХарактеристикаНоменклатуры,
		|	ВТ_Материалы.СерияНоменклатуры,
		|	ВТ_Материалы.ДокументПередачи,			
		|	ВТ_Материалы.ФизЛицо,
		|	ВТ_Материалы.НазначениеИспользования,
		|	ВТ_Материалы.СпособПогашенияСтоимости,
		|	ВТ_Материалы.СрокПолезногоИспользования,
		|	ВТ_Материалы.Количество,
		|	ВТ_Материалы.Сумма,
		|	ВТ_Материалы.Организация,
		|	&Подразделение,
		|	ВТ_Материалы.СчетУчета,
		|	ВТ_Материалы.НомерСтроки КАК НомерСтроки
		|ИЗ
		|	ВТ_Материалы КАК ВТ_Материалы
		|
		|УПОРЯДОЧИТЬ ПО
		|	НомерСтроки";
		Выборка = Запрос.Выполнить().Выбрать();
		
		НаборДвижений = Движения["ПартииМатериаловВЭксплуатации" + РазделУчета.Значение];
		
		//Предполагается что все ресурсы кроме количества распределяются пропорционально количеству
		ДанныеРаспределения = Новый Массив;
		Для каждого Ресурс Из НаборДвижений.Метаданные().Ресурсы Цикл
			Если Ресурс.Имя = "Количество" Тогда
				Продолжить;
			КонецЕсли; 
			ДанныеРаспределения.Добавить(Ресурс.Имя);
		КонецЦикла; 
		
		Пока Выборка.Следующий() Цикл
			
			Если ЗначениеЗаполнено(Выборка.ДокументПередачи) Тогда
				СтруктураИзмерений = Новый Структура("Номенклатура, ХарактеристикаНоменклатуры, СерияНоменклатуры, НазначениеИспользования, ФизЛицо, Подразделение, Организация, ДокументПередачи");
			Иначе
				СтруктураИзмерений = Новый Структура("Номенклатура, ХарактеристикаНоменклатуры, СерияНоменклатуры, НазначениеИспользования, ФизЛицо, Подразделение, Организация");
			КонецЕсли;
		
			ЗаполнитьЗначенияСвойств(СтруктураИзмерений, Выборка);
			ОстатокСтроки = ОстаткиПоРегистру.НайтиСтроки(СтруктураИзмерений);
			КоличествоОсталосьСписать = Выборка.Количество;
			
			Для Каждого Остаток Из ОстатокСтроки Цикл
				
				Если КоличествоОсталосьСписать <= 0 ИЛИ Остаток.КоличествоОстаток = 0 Тогда
					Продолжить;
				КонецЕсли;
				
				НовоеДвижение = НаборДвижений.ДобавитьРасход();
				
				ЗаполнитьЗначенияСвойств(НовоеДвижение, Выборка);
				
				НовоеДвижение.Период		   = СтруктураШапкиДокумента.Дата;
				НовоеДвижение.Регистратор	   = СтруктураШапкиДокумента.Ссылка;
				НовоеДвижение.ДокументПередачи = Остаток.ДокументПередачи;
				
				НовоеДвижение.Количество = КоличествоОсталосьСписать;
				НовоеДвижениеКоличество  = КоличествоОсталосьСписать;				
				
				//Распределим сумму и количество
				Для каждого ЭлементРаспределения Из ДанныеРаспределения Цикл
					
					Если Остаток.КоличествоОстаток = НовоеДвижение.Количество Тогда
						
						НовоеДвижение[ЭлементРаспределения] = Остаток[ЭлементРаспределения + "Остаток"];
						
					ИначеЕсли Остаток.КоличествоОстаток <> 0 Тогда
						
						Если Остаток.КоличествоОстаток >=  НовоеДвижение.Количество Тогда
							Коэффициент = НовоеДвижение.Количество / Остаток.КоличествоОстаток;
							НовоеДвижение[ЭлементРаспределения] = Остаток[ЭлементРаспределения + "Остаток"]  * Коэффициент;
						Иначе
							НовоеДвижениеКоличество = Остаток.КоличествоОстаток;
							Коэффициент = 1;
							НовоеДвижение[ЭлементРаспределения] = Остаток[ЭлементРаспределения + "Остаток"]  * Коэффициент;							
						КонецЕсли;
						
					Иначе
						
						НовоеДвижениеКоличество             = 0;
						НовоеДвижение[ЭлементРаспределения] = 0;
						
					КонецЕсли;
					
					Остаток[ЭлементРаспределения + "Остаток"] = Остаток[ЭлементРаспределения + "Остаток"] - НовоеДвижение[ЭлементРаспределения];
					
				КонецЦикла;
				
				НовоеДвижение.Количество  = НовоеДвижениеКоличество;
				Остаток.КоличествоОстаток = Остаток.КоличествоОстаток - НовоеДвижение.Количество;
				КоличествоОсталосьСписать = КоличествоОсталосьСписать - НовоеДвижение.Количество;
				
				НовоеДвижение.СписаниеПартий = Истина;
				НовоеДвижение.ДокументДвижения			= СтруктураШапкиДокумента.Ссылка;
				НовоеДвижение.ДокументДвиженияПериод	= СтруктураШапкиДокумента.Дата;
				НовоеДвижение.НомерКорСтроки			= Выборка.НомерСтроки;
				
				
				//Произведем корректировку погашеной стоимости
				Если Не ЭтоНУ
					И Выборка.СпособПогашенияСтоимости = Перечисления.СпособыПогашенияСтоимости.Линейный
					И Выборка.СрокПолезногоИспользования > 0
					И НовоеДвижение.ПогашеннаяСтоимость < НовоеДвижение.Стоимость Тогда
					
					СуммаСписания = НовоеДвижение.Стоимость / Выборка.СрокПолезногоИспользования;
					
					НовоеДвижение.ПогашеннаяСтоимость = Мин(НовоеДвижение.ПогашеннаяСтоимость + СуммаСписания, НовоеДвижение.Стоимость);
					
				КонецЕсли; 
				
			КонецЦикла;
			
			Если КоличествоОсталосьСписать > 0 Тогда			
				СтрокаСообщения = "Для заданного набора ";				
				Для Каждого Измерение Из СтруктураИзмерений Цикл
					Если НЕ ЗначениеЗаполнено(Измерение) Тогда
						Продолжить;
					КонецЕсли;
					СтрокаСообщения = СтрокаСообщения + " ["+Измерение.Ключ+"] " + Измерение.Значение;
				КонецЦикла;
				
				СтрокаСообщения = СтрокаСообщения + " недостаточно остатков по регистру ""Партии материалов в эксплуатации " + РазделУчета.Значение + """""";
				ttk_ОбщегоНазначения.СообщитьОбОшибке(СтрокаСообщения, Отказ);
			КонецЕсли;
		КонецЦикла;
		
	КонецЦикла;	
	
КонецПроцедуры

