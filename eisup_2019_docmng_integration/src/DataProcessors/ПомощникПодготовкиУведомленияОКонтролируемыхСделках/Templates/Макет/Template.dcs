ВЫБРАТЬ
	ВТ.Ссылка,
	ВТ.Контрагент,
	ВТ.ДоговорКонтрагента,
	ВТ.СуммаДоходов,
	ВТ.СуммаРасходов,
	ВТ.КодНаименованияСделки,
	ВТ.КодСтраныПроисхождения,
	ВТ.КодТНВЭД,
	ВТ.КодОКП,
	ВТ.КодОКВЭД,
